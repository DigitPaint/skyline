<%- @options = {
  "locale"  => I18n.locale.to_s.downcase
} -%>
<% @locale.each do |key,value| %>
  tinyMCE.addI18n("<%= I18n.locale %>.<%= key %>",
    <%= value.update(@options).to_json %>
  );
<% end %>

I18n = {};
